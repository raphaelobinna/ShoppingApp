    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.13",
    "multer": "^1.4.2",
    "react-redux": "^7.2.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
